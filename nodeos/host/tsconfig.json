{
  "compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "amd",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "outFile": "../../www/bin/index.js",
    "sourceMap": false,
    "strict": true,
    "target": "ES2020",
    "typeRoots": [
      "xterm"
    ],
    "lib": [
      "ES2020",
      "dom",
    ],
    "newLine": "LF"
  }
}